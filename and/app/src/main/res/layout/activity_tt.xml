                style="@style/TEXT_STYLE_1"
                android:layout_width="60dp"
                android:layout_height="50dp"/>
            <TextView
                android:layout_weight="1"
                android:gravity="center"
                style="@style/TEXT_STYLE_1"
                android:text="월"
                android:layout_width="0dp"
                android:layout_height="50dp"/>
            <TextView
                android:layout_weight="1"
                android:gravity="center"
                style="@style/TEXT_STYLE_1"
                android:text="화"
                android:layout_width="0dp"
                android:layout_height="50dp"/>
            <TextView
                android:layout_weight="1"
                android:gravity="center"
                style="@style/TEXT_STYLE_1"
                android:text="수"
                android:layout_width="0dp"
                android:layout_height="50dp"/>
            <TextView
                android:layout_weight="1"
                android:gravity="center"
                style="@style/TEXT_STYLE_1"
                android:text="목"
                android:layout_width="0dp"
                android:layout_height="50dp"/>
