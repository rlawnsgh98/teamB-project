package test;

public class TestVO {
	private int iVal;
	private String sVal;
	private double dVal;
	
	public TestVO() {}
	
	public TestVO(int iVal, String sVal, double dVal) {
		super();
		this.iVal = iVal;
		this.sVal = sVal;
		this.dVal = dVal;
	}
	public int getiVal() {
		return iVal;
	}
	public void setiVal(int iVal) {
		this.iVal = iVal;
	}
	public String getsVal() {
		return sVal;
	}
	public void setsVal(String sVal) {
		this.sVal = sVal;
	}
	public double getdVal() {
		return dVal;
	}
	public void setdVal(double dVal) {
		this.dVal = dVal;
	}
	
}
